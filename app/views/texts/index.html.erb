<button type="button" class="btn btn-primary">Primary</button>
<h1>投稿一覧</h1>
<table>
  <thead>
    <tr>
      <th scope="col">No.</th>
      <th scope="col">タイトル</th>
      <th scope="col"></th>
    </tr>
  </thead>
  <tbody>
    <% @texts.each.with_index(1) do |texts, i| %>
      <tr>
        <th scope="row"><%= i %></th>
        <td><%= texts.title %></td>
        <td><%= texts.genre %></td>
      </tr>
    <% end %>
  </tbody>
</table>
